{
    "name": "jbouzekri/composer-deploy-plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "Auto deploy static files with composer in a public document root folder",
    "keywords": ["php", "composer", "continuous integration", "deployment"],
    "homepage": "https://github.com/jbouzekri/ComposerDeployPlugin",
    "require": {
        "composer-plugin-api": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Jb\\Composer\\DeployPlugin\\": "src/Jb/Composer/DeployPlugin"
        }
    },
    "extra": {
        "class": "Jb\\Composer\\DeployPlugin\\JbDeployPlugin"
    },
    "authors": [
        {
            "name": "Jonathan Bouzekri",
            "email": "jonathan.bouzekri@gmail.com",
            "homepage": "http://jonathan.bouzekri.net"
        }
    ]
}